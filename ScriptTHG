-- CHECK KEY
local keyOk = false
CheckButton.MouseButton1Click:Connect(function()
    local userKey = TextBox.Text
    local correct = false

    for _, k in ipairs(validKeys) do
        if userKey == k then
            correct = true
            break
        end
    end

    if correct == true then
        keyOk = true
        ScreenGui:Destroy()
        game.StarterGui:SetCore("SendNotification", {
            Title = "Th√†nh c√¥ng",
            Text = "Key ƒë√∫ng! Script s·∫Ω kh·ªüi ch·∫°y.",
            Duration = 3
        })
